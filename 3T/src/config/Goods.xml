<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">      

<struts>    
<package name="3T" extends="struts-default">

	<result-types>
		<result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult"></result-type>
	</result-types>
	
	<!-- main -->
	<action name="main" class="board.mainAction">
		<result name="success" type="tiles">
		main
		</result>
	</action>
	
	<action name="list_outer" class="board.mainAction" method="outer">
		<result name="success" type="tiles">
		list_outer
		</result>
	</action>

</package>    
</struts>